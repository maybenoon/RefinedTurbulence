FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 33;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 33;
    }

    outlet
    {
        type            zeroGradient;
    }

    sides
    {
        type            symmetry;
    }

    bottom
    {
        type            omegaWallFunction;
        value           uniform 33;
    }

    hull
    {
        type            omegaWallFunction;
        value           uniform 33;
    }

    atmosphere
    {
        type            inletOutlet;
        inletValue      uniform 33;
        value           uniform 33;
    }
}
