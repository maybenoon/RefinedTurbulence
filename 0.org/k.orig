FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 2.57e-4;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 2.57e-4;
    }

    outlet
    {
        type            zeroGradient;
    }

    sides
    {
        type            symmetry;
    }

    bottom
    {
        type            kqRWallFunction;
        value           uniform 2.57e-4;
    }

    hull
    {
        type            kqRWallFunction;
        value           uniform 2.57e-4;
    }

    atmosphere
    {
        type            inletOutlet;
        inletValue      uniform 2.57e-4;
        value           uniform 2.57e-4;
    }
}
